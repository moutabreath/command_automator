{
    "containers": {
        "job_container": [".jobCard", "div[class*='jobCard']", "[class*='JobCard']"], 
        "job_card": ".jobCard"
    },
    "stats": {
        "overview": ".stats p",
        "location_filters": ".location-filter .filter-tag"
    },
    "job_details": {
        "title": {
            "selector": "a[class^='JobCard_jobTitle__'], a[data-test='job-title']",
            "attribute": "inner_text"
        },
        "company": {
            "selector": "span[class^='EmployerProfile_compactEmployerName__'], [data-test='employer-name']",
            "attribute": "inner_text"
        },
        "location": {
            "selector": "div[class^='JobCard_location__'], [data-test='emp-location']",
            "attribute": "inner_text"
        },
        "url": {
            "selector": "a[class^='JobCard_jobTitle__'], a[data-test='job-title']",
            "attribute": "href"
        },
        "company_rating": {
            "selector": "span[class^='rating-single-star_RatingText__'], [data-test='rating']",
            "attribute": "inner_text"
        },
        "company_logo": {
            "selector": "img[class^='avatar-base_Image__']",
            "attribute": "src"
        },
        "description": {
            "selector": "div[class^='JobCard_jobDescriptionSnippet__'] div, [data-test='descSnippet'] div",
            "attribute": "inner_text"
        },
        "posted_date": {
            "selector": "div[class^='JobCard_listingAge__'], [data-test='job-age']",
            "attribute": "inner_text"
        },
        "job_id": {
            "selector": "a[class^='JobCard_jobTitle__'], a[data-test='job-title']",
            "attribute": "id"
        },
        "tracking_link": {
            "selector": "a[class^='JobCard_trackingLink__']",
            "attribute": "href"
        }
    },
    "popups": {
        "signup_modal": "[data-test=\"modal-close\"], .modal-close, [aria-label=\"Close\"], .CloseButton",
        "cookie_consent": "#onetrust-accept-btn-handler, .cookie-accept, [id*='cookie'] button",
        "opportunity_popup": ".CloseButton, [aria-label=\"Close\"], .modal-close"
    }
}